@{
    ViewData["Title"] = "Создание сертификата";
}
<!DOCTYPE html>

<html>
<head>
    <title>Создание сертификата</title>
</head>
<body>
    <h3>Создание сертификата</h3>
    <form method="post" action="~/Certificate/Create">
        <div class="form-group">
            <label>ФИО</label>
            <input type="text" name="fio"/>
        </div>

        <div class="form-group">
            <label>Остальное</label>
            <input type="text" name="other" />
        </div>

        <button type="submit" class="btn btn-primary">Создать</button>
    </form>
</body>
</html>
